{
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
  "builds": [
    {
        "src": "api/dist/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
      {
          "src": "api/(.*)",
          "dest": "api/dist/index.js"
      }
  ]
}
